	.data
newline:	 .asciiz "\n"
	.text
save_frame:
	addi $sp, $sp, -44
	sw $t0, 44($sp)
	sw $t1, 40($sp)
	sw $t2, 36($sp)
	sw $t3, 32($sp)
	sw $t4, 28($sp)
	sw $t5, 24($sp)
	sw $t6, 20($sp)
	sw $t7, 16($sp)
	sw $t8, 12($sp)
	sw $t9, 8($sp)
	sw $fp, 4($sp)
	jr $ra
load_frame:
	lw $t0, 44($sp)
	lw $t1, 40($sp)
	lw $t2, 36($sp)
	lw $t3, 32($sp)
	lw $t4, 28($sp)
	lw $t5, 24($sp)
	lw $t6, 20($sp)
	lw $t7, 16($sp)
	lw $t8, 12($sp)
	lw $t9, 8($sp)
	lw $fp, 4($sp)
	addi $sp, $sp, 44
	jr $ra
func:
	sw $ra, 0($fp)
	lw $ra, 0($fp)
	li $v1, 0
	move $sp, $fp
	jr $ra
main:
	move $fp, $sp
	jal save_frame
	move $v1, $sp
	addi $sp, $sp, -4
	li $t1, 3
	addi $sp, $sp, -4
	sw $t1, 4($sp)
	li $t2, 4
	addi $sp, $sp, -4
	sw $t2, 4($sp)
	li $t3, 5
	addi $sp, $sp, -4
	sw $t3, 4($sp)
	move $fp, $v1
	jal func
	jal load_frame
	li $v0, 10
	syscall
