	.data
newline:	 .asciiz "\n"
	.text
save_frame:
	addi $sp, $sp, -44
	sw $t0, 44($sp)
	sw $t1, 40($sp)
	sw $t2, 36($sp)
	sw $t3, 32($sp)
	sw $t4, 28($sp)
	sw $t5, 24($sp)
	sw $t6, 20($sp)
	sw $t7, 16($sp)
	sw $t8, 12($sp)
	sw $t9, 8($sp)
	sw $fp, 4($sp)
	jr $ra
load_frame:
	lw $t0, 44($sp)
	lw $t1, 40($sp)
	lw $t2, 36($sp)
	lw $t3, 32($sp)
	lw $t4, 28($sp)
	lw $t5, 24($sp)
	lw $t6, 20($sp)
	lw $t7, 16($sp)
	lw $t8, 12($sp)
	lw $t9, 8($sp)
	lw $fp, 4($sp)
	addi $sp, $sp, 44
	jr $ra
main:
	move $fp, $sp
	addi $sp, $sp, -8
	li $t1, 5
	sw $t1, -4($fp)
	lw $t2, -4($fp)
	move $a0, $t2
	li $v0, 1
	syscall
	la $a0, newline
	li $v0, 4
	syscall
	li $v0, 10
	syscall
